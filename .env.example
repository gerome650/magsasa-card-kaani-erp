# Environment Configuration Template
# Copy this file to .env and fill in your values

# Database
DATABASE_URL=mysql://user:password@localhost:3306/magsasa_card

# Google AI Studio (for KaAni chatbot)
GOOGLE_AI_STUDIO_API_KEY=your_api_key_here

# JWT Authentication
JWT_SECRET=your_secret_key_here

# Deployment Profile (controls partner-specific policies and UI visibility)
# Valid values: CARD_MRI (UI visible), LANDBANK (internal only), DEV (UI visible)
# Defaults to DEV if not specified
DEPLOYMENT_PROFILE=DEV

# Audit Logging Configuration
# Salt for hashing farmer IDs in audit logs (never log raw farmerId)
LOG_HASH_SALT=your_log_hash_salt_here

# Enable client-side audit logs for loan suggestion rendering (optional)
# Set to "true" to enable UI audit logs (only logs when visibility === "ui")
VITE_ENABLE_UI_AUDIT_LOGS=false
